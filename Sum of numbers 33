import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner s=new Scanner(System.in);
    int a,sum=0;
        a=s.nextInt();
      while(a!=-1)
   {
       sum=sum+a;
          a=s.nextInt();
   }
     System.out.printf("%d",sum);
    }
}

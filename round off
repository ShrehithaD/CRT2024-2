import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
    float a=s.nextFloat();
    System.out.println((int)a);
    float b=(float)Math.ceil(a);
    float c=(float)Math.floor(a);
    System.out.printf("%.1f\n%.1f",b,c);
    
    }
}
